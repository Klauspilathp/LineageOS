  <process id="evectionProcess" name="出差流程" isExecutable="true">
    <documentation>员工出差流程</documentation>
    <startEvent id="startevent" name="Start" activiti:initiator="staffUserId"></startEvent>
    <userTask id="usertask1" name="领取出差申请表"></userTask>
    <sequenceFlow id="flow1" name="找人事" sourceRef="startevent" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask2" name="提交出差申请"></userTask>
    <sequenceFlow id="flow2" name="填写出差申请表" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <userTask id="usertask3" name="项目经理">
      <extensionElements>
        <activiti:taskListener event="create" class="com.d7c.springboot.client.services.evection.listener.PMListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="usertask2" targetRef="usertask3"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" name="同意" sourceRef="usertask3" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="usertask4" name="总经理"></userTask>
    <sequenceFlow id="flow8" name="大于7天" sourceRef="exclusivegateway1" targetRef="usertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${evection.days >= 7}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow4" name="不同意" sourceRef="usertask3" targetRef="endevent2"></sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow9" name="不同意" sourceRef="usertask4" targetRef="endevent3"></sequenceFlow>
    <userTask id="usertask5" name="人事"></userTask>
    <sequenceFlow id="flow7" name="小于7天" sourceRef="exclusivegateway1" targetRef="usertask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${evection.days < 7}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent4" name="End"></endEvent>
    <sequenceFlow id="flow11" sourceRef="usertask5" targetRef="endevent4"></sequenceFlow>
    <sequenceFlow id="flow10" name="同意" sourceRef="usertask4" targetRef="usertask5"></sequenceFlow>
    <sequenceFlow id="flow5" name="驳回重填" sourceRef="usertask3" targetRef="usertask2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow12" name="不出差了" sourceRef="usertask2" targetRef="endevent1"></sequenceFlow>
  </process>
